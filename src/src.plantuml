  }
  

  package  {
    class FullRights {
        + FullRights()
    }
  }
  

  package  {
    class MediaDoc {
        - imageDimensionHeight : double
        - imageDimensionWidth : double
        + MediaDoc()
    }
  }
  

  package  {
    class PdfDoc {
        + PdfDoc()
    }
  }
  

  package  {
    class PictureDoc {
        + PictureDoc()
    }
  }
  

  package  {
    class Program {
        {static} + main()
    }
  }
  

  package  {
    class ReadOnly {
        + ReadOnly()
    }
  }
  

  package  {
    class TextDoc {
        + TextDoc()
    }
  }
  

  package  {
    class VideoDoc {
        - videoDuration : double
        + VideoDoc()
    }
  }
  

  package  {
    class WordDoc {
        + WordDoc()
    }
  }
  

